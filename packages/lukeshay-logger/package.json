{
  "license": "MIT",
  "name": "@lukeshay/logger",
  "version": "0.3.2",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "engines": {
    "npm": ">=7",
    "node": ">=16"
  },
  "scripts": {
    "analyze": "size-limit --why",
    "build": "../../scripts/build.sh --target node",
    "lint": "../../scripts/lint.sh",
    "lint:fix": "npm run lint -- --fix",
    "clean": "../../scripts/clean.sh",
    "prepack": "npm run build",
    "size": "size-limit",
    "test": "../../scripts/test.sh",
    "watch": "../../scripts/watch-web.sh"
  },
  "author": "Luke Shay <https://lukeshay.com>",
  "size-limit": [
    {
      "path": "dist/index.cjs.production.min.js",
      "limit": "350 KB",
      "gzip": false,
      "ignore": [
        "fs",
        "os",
        "path",
        "http",
        "https",
        "zlib"
      ]
    },
    {
      "path": "dist/index.esm.js",
      "limit": "350 KB",
      "gzip": false,
      "ignore": [
        "fs",
        "os",
        "path",
        "http",
        "https",
        "zlib"
      ]
    }
  ],
  "dependencies": {
    "winston": "3.4.0"
  },
  "prettier": "../../.prettierrc.cjs",
  "jest": {
    "preset": "../../jest.config.cjs"
  },
  "devDependencies": {
    "@types/chance": "1.1.3",
    "chance": "1.1.8"
  }
}
