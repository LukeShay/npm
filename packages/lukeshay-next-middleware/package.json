    {
      "path": "dist/index.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "next": "^12.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "http-status-codes": "^2.1.4"
